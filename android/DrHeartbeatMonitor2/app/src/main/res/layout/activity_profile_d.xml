<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#6699ff"
    android:id="@+id/drawer_layout1"
    tools:context=".ProfileD">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <include
            layout="@layout/toolbar_main1"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp">
        <RelativeLayout
            android:layout_marginStart="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/full_name1"
                android:textColor="#fff"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <EditText
                android:id="@+id/full_name0"
                android:textSize="12sp"
                android:textColor="#fff"
                android:layout_marginStart="106dp"
                android:layout_width="78dp"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"/>
            <EditText
                android:id="@+id/full_name11"
                android:textSize="12sp"
                android:textColor="#fff"
                android:layout_marginStart="183dp"
                android:layout_width="78dp"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent" />
            <EditText
                android:id="@+id/full_name2"
                android:textColor="#fff"
                android:background="@android:color/transparent"
                android:textSize="12sp"
                android:layout_marginStart="263dp"
                android:layout_width="78dp"
                android:layout_height="wrap_content" />

        </RelativeLayout>
        <RelativeLayout
            android:layout_marginStart="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/Phone_ed1"
                android:textColor="#fff"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"/>
            <EditText
                android:id="@+id/Phone_ed11"
                android:textColor="#fff"
                android:layout_marginStart="150dp"
                android:layout_width="200dp"
                android:textSize="12sp"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginStart="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/Email_ed1"
                android:text="@string/space"
                android:textColor="#fff"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <EditText
                android:id="@+id/Email_ed11"
                android:textColor="#fff"
                android:textSize="12sp"
                android:background="@android:color/transparent"
                android:layout_marginStart="150dp"
                android:layout_width="200dp"
                android:layout_height="wrap_content" />

        </RelativeLayout>
        <RelativeLayout
            android:layout_marginStart="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/Address1"
                android:text="@string/space"
                android:textColor="#fff"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <EditText
                android:id="@+id/Address11"
                android:textColor="#fff"
                android:textSize="12sp"
                android:layout_marginStart="150dp"
                android:background="@android:color/transparent"
                android:layout_width="200dp"
                android:layout_height="wrap_content" />
        </RelativeLayout>
        <Button
            android:id="@+id/edit_info1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="250dp"
            android:textSize="12sp"
            android:text="@string/edit_information" />
    </LinearLayout>
    </LinearLayout>
    <RelativeLayout
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:layout_gravity="start">
        <include
            layout="@layout/nav_drawer_main1"/>
    </RelativeLayout>
</androidx.drawerlayout.widget.DrawerLayout>